{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "double", { "allowTemplateLiterals": true }],
    "comma-dangle": "off",
    "mplicit-arrow-linebreak": "off",
    " function-paren-newline": "multiline"
  }
}
